  country         = {United States},
  issn-linking    = {0027-8424},
  keywords        = {Basic Reproduction Number; Betacoronavirus; Communicable Disease Control, methods; Coronavirus Infections, epidemiology, prevention & control, transmission; Hospitalization, statistics & numerical data; Humans; Italy, epidemiology; Models, Theoretical; Pandemics, prevention & control; Pneumonia, Viral, epidemiology, prevention & control, transmission; SARS-CoV-2; SEIR models; disease outbreak scenarios; social contact restrictions; spatially explicit epidemiology},
