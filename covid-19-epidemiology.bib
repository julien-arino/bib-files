@Article{KwokChanHuangHuiEtAl2020,
  author          = {Kwok, K. O. and Chan, H. H. H. and Huang, Y. and Hui, D. S. C. and Tambyah, P. A. and Wei, W. I. and Chau, P. Y. K. and Wong, S. Y. S. and Tang, J. W. T.},
