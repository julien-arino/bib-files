  country         = {Canada},
  issn-linking    = {0820-3946},
  keywords        = {Bed Occupancy, statistics & numerical data; Canada, epidemiology; Coronavirus Infections, epidemiology, therapy; Critical Care, statistics & numerical data; Disease Outbreaks; Health Services Needs and Demand, statistics & numerical data; Hospital Bed Capacity, statistics & numerical data; Humans; Models, Statistical; Pandemics; Pneumonia, Viral, epidemiology, therapy},
