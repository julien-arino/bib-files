@Article{WangLanderasBuenoHsiehTeradaEtAl2020,
  author          = {Wang, Ying-Ting and Landeras-Bueno, Sara and Hsieh, Li-En and Terada, Yutaka and Kim, Kenneth and Ley, Klaus and Shresta, Sujan and Saphire, Erica Ollmann and Regla-Nava, Jose Angel},
