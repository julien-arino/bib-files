@Article{MishraMohapatraKumarSinghEtAl2020,
  author       = {Mishra, Surabhi and Mohapatra, Archisman and Kumar, Raman and Singh, Anjana and Bhadoria, Ajeet Singh and Kant, Ravi},
  date         = {2020-04},
  journaltitle = {Journal of Family Medicine and Primary Care},
  title        = {Restricting rural-urban connect to combat infectious disease epidemic as {I}ndia fights {COVID}-19.},
