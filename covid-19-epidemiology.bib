  pmid            = {32697418},
  pubmodel        = {Print-Electronic},
  revised         = {2020-09-09},
}

@Article{AdigaVenkatramananSchlittPeddireddyEtAl2020,
  author       = {Adiga, Aniruddha and Venkatramanan, Srinivasan and Schlitt, James and Peddireddy, Akhil and Dickerman, Allan and Bura, Andrei and Warren, Andrew and Klahn, Brian D. and Mao, Chunhong and Xie, Dawen and Machi, Dustin and Raymond, Erin and Meng, Fanchao and Barrow, Golda and Mortveit, Henning and Chen, Jiangzhuo and Walke, Jim and Goldstein, Joshua and Wilson, Mandy L. and Orr, Mark and Porebski, Przemyslaw and Telionis, Pyrros A. and Beckman, Richard and Hoops, Stefan and Eubank, Stephen and Baek, Young Yun and Lewis, Bryan and Marathe, Madhav and Barrett, Chris},
