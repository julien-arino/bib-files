  issn-linking    = {1078-8956},
  keywords        = {Adolescent; Adult; Age Factors; Aged; Betacoronavirus, pathogenicity; Child; Comorbidity; Coronavirus Infections, epidemiology, transmission, virology; Epidemics; Female; Humans; Models, Theoretical; Pandemics; Pneumonia, Viral, epidemiology, transmission, virology; Young Adult},
