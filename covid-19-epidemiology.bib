@Article{ColizzaBarthelemyBarratVespignani2007,
  author          = {Colizza, Vittoria and Barthélemy, Marc and Barrat, Alain and Vespignani, Alessandro},
  date            = {2007-04},
  journaltitle    = {Comptes rendus Biologies},
