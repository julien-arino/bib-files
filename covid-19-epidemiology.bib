  journal         = {Epidemiology and infection},
  keywords        = {Asymptomatic Infections, epidemiology; China, epidemiology; Contact Tracing; Coronavirus Infections, epidemiology, transmission; Female; Humans; Male; Pandemics; Pneumonia, Viral, epidemiology, transmission; Asymptomatic carrier; COVID-19; SARS-CoV-2; infectivity of asymptomatic; proportion of asymptomatic},
