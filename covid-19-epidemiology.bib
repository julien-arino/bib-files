@Article{MassadAmakuWilderSmithCostaDosSantosEtAl2020,
  author          = {Massad, Eduardo and Amaku, Marcos and Wilder-Smith, Annelies and Costa Dos Santos, Paulo Cesar and Struchiner, Claudio Jose and Coutinho, Francisco Antonio Bezerra},
  date            = {2020-06},
  journaltitle    = {Epidemiology \& Infection},
  title           = {Two complementary model-based methods for calculating the risk of international spreading of a novel virus from the outbreak epicentre. The case of COVID-19.},
