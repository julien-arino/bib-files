  revised         = {2020-08-25},
}

@Article{SaviniCandeloroCalistriConte2020,
  author       = {Savini, Lara and Candeloro, Luca and Calistri, Paolo and Conte, Annamaria},
  date         = {2020-06},
  journaltitle = {Microorganisms},
  title        = {A Municipality-Based Approach Using Commuting Census Data to Characterize the Vulnerability to Influenza-Like Epidemic: The COVID-19 Application in Italy.},
