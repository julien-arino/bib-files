  country         = {England},
  issn-linking    = {0022-5193},
  keywords        = {Algorithms; Cholera, epidemiology, microbiology, transmission; Communicable Diseases, epidemiology, transmission; Cost-Benefit Analysis; Disaster Planning, economics, methods; Disasters, economics, prevention & control; Drinking Water, microbiology; Epidemics, prevention & control; Food Microbiology; Host-Pathogen Interactions; Humans; Models, Theoretical; Population Density; Prevalence; Vibrio cholerae, physiology; Behaviour; Diarrhoea; Environmental transmission; Epidemic; Mathematical model; Metapopulation; Natural disaster},
  nlm-id          = {0376342},
