@Article{SoodAggarwalAggarwalUpadhyayEtAl2020,
  author       = {Sood, Shivani and Aggarwal, Vaishali and Aggarwal, Diwakar and Upadhyay, Sushil K. and Sak, Katrin and Tuli, Hardeep Singh and Kumar, Manoj and Kumar, Jayant and Talwar, Shivangi},
