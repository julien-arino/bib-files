@Article{ZhangSchwartz2020,
  author          = {Zhang, Charlie H. and Schwartz, Gary G.},
  date            = {2020-06},
  journaltitle    = {The Journal of rural health : official journal of the American Rural Health Association and the National Rural Health Care Association},
  title           = {Spatial Disparities in Coronavirus Incidence and Mortality in the United States: An Ecological Analysis as of May 2020.},
