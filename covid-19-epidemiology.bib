  country         = {England},
  issn-linking    = {0896-8411},
  keywords        = {Animals; Betacoronavirus, genetics, pathogenicity; China, epidemiology; Clinical Laboratory Techniques; Coronavirus Infections, diagnosis, drug therapy, economics, epidemiology, prevention & control; Genome, Viral; History, 21st Century; Humans; Information Dissemination; Pandemics, economics, prevention & control; Pneumonia, Viral, diagnosis, economics, epidemiology, prevention & control; Pyroptosis; Quarantine; Severe Acute Respiratory Syndrome, history, prevention & control; Zoonoses, virology; Bats; Coronavirus; Epidemic; Epidemiology; Flu; Human to human transmission; Pandemic; Pneumonia; Pyroptosis; SARS-CoV; SARS-CoV-2},
