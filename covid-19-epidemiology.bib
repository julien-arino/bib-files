  country         = {United States},
  issn-linking    = {1055-9965},
  keywords        = {Betacoronavirus; Coronavirus Infections, diagnosis, epidemiology; Data Collection, methods; Humans; Models, Biological; Pandemics; Pneumonia, Viral, diagnosis, epidemiology; Public Health; Smartphone; Software; United Kingdom, epidemiology; United States, epidemiology},
  mid             = {NIHMS1605601},
