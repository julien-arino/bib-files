  author          = {Gomes, D. S. and Andrade, L. A. and Ribeiro, C. J. N. and Peixoto, M. V. S. and Lima, S. V. M. A. and Duque, A. M. and Cirilo, T. M. and Góes, M. A. O. and Lima, A. G. C. F. and Santos, M. B. and Araújo, K. C. G. M. and Santos, A. D.},
  date            = {2020-08},
  journaltitle    = {Epidemiology \& Infection},
  title           = {Risk clusters of COVID-19 transmission in northeastern Brazil: prospective space-time modelling.},
  doi             = {10.1017/S0950268820001843},
