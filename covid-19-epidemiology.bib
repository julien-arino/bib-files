@Article{MiyachiTanimotoKami2020,
  author          = {Miyachi, T. and Tanimoto, T. and Kami, M.},
  date            = {2020-06},
  journaltitle    = {Epidemiology \& Infection},
  title           = {Evaluation of modelling study shows limits of COVID-19 importing risk simulations in sub-Saharan Africa.},
