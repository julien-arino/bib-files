@Article{KalbuschHenningBrikalskiLucaEtAl2020,
  author       = {Kalbusch, Andreza and Henning, Elisa and Brikalski, Miqueias Paulo and Luca, Felipe Vieira de and Konrath, Andrea Cristina},
  date         = {2020-12},
  journaltitle = {Resources, conservation, and recycling},
  title        = {Impact of coronavirus (COVID-19) spread-prevention actions on urban water consumption.},
