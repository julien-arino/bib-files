  keywords        = {Africa, Eastern, epidemiology; Cholera, epidemiology; Disease Outbreaks; Fisheries; Humans; Incidence; Phytoplankton; Prevalence; Seasons; Tropical Climate; Vibrio cholerae},
  nlm-id          = {9508155},
