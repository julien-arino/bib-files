  date            = {2020-07},
  journaltitle    = {International journal of environmental research and public health},
  title           = {A Systems Approach to Assess Transport and Diffusion of Hazardous Airborne Particles in a Large Surgical Suite: Potential Impacts on Viral Airborne Transmission.},
  doi             = {10.3390/ijerph17155404},
