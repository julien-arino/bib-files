@Article{LiuXuWangWang2020,
  author          = {Liu, Qin and Xu, Kaiyuan and Wang, Xiang and Wang, Wenmei},
  date            = {2020-08},
  journaltitle    = {Journal of infection and public health},
  title           = {From SARS to COVID-19: What lessons have we learned?},
