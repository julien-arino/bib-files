  country         = {England},
  issn-linking    = {2045-2322},
  keywords        = {Epidemics; France, epidemiology; Geography; Humans; Incidence; Influenza, Human, epidemiology, transmission; Probability; Seasons; Time Factors},
  nlm-id          = {101563288},
