  country         = {England},
  issn-linking    = {2397-3374},
  keywords        = {Betacoronavirus; Boston, epidemiology; Clinical Laboratory Techniques, statistics & numerical data; Contact Tracing, statistics & numerical data; Coronavirus Infections, diagnosis, epidemiology, prevention & control; Family Characteristics; Hospitalization, statistics & numerical data; Humans; Infection Control, methods, statistics & numerical data; Models, Statistical; Pandemics, prevention & control, statistics & numerical data; Pneumonia, Viral, diagnosis, epidemiology, prevention & control},
