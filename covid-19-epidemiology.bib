  country         = {United States},
  issn-linking    = {0027-8424},
  keywords        = {Adolescent; Adult; Aged; Asymptomatic Infections, epidemiology; Betacoronavirus, isolation & purification; Child; Child, Preschool; Contact Tracing, statistics & numerical data; Coronavirus Infections, epidemiology, prevention & control, transmission, virology; Female; Humans; Incidence; Infant; Infant, Newborn; Infection Control, methods; Male; Middle Aged; Pandemics, prevention & control; Pneumonia, Viral, epidemiology, prevention & control, transmission, virology; Quarantine, statistics & numerical data; Young Adult; COVID-19; case isolation; contact tracing},
