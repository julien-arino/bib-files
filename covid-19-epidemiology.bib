  issn-linking    = {0036-8075},
  keywords        = {Betacoronavirus; China, epidemiology; Communicable Diseases, Imported, epidemiology, prevention & control, transmission; Computer Simulation; Coronavirus Infections, epidemiology, prevention & control, transmission; Disease Outbreaks; Humans; Incidence; Internationality; Models, Statistical; Pandemics, prevention & control; Pneumonia, Viral, epidemiology, prevention & control, transmission; Quarantine; Travel},
