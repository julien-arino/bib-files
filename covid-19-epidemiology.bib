  author          = {Uddin, Mohammed and Mustafa, Farah and Rizvi, Tahir A. and Loney, Tom and Suwaidi, Hanan Al and Al-Marzouqi, Ahmed H. Hassan and Eldin, Afaf Kamal and Alsabeeha, Nabeel and Adrian, Thomas E. and Stefanini, Cesare and Nowotny, Norbert and Alsheikh-Ali, Alawi and Senok, Abiola C.},
