  date            = {2020-08},
  journaltitle    = {The Science of the Total Environment},
  title           = {A spatio-temporal analysis for exploring the effect of temperature on {COVID}-19 early evolution in Spain.},
  doi             = {10.1016/j.scitotenv.2020.138811},
