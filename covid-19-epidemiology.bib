  issn-linking    = {2045-2322},
  keywords        = {Adult; Child; Coinfection, epidemiology, transmission; Community-Acquired Infections, epidemiology, microbiology, transmission; Cross Reactions; Epidemics; Female; Humans; Incidence; Male; Models, Biological; Mycoplasma pneumoniae, immunology; Periodicity; Pneumonia, Mycoplasma, epidemiology, microbiology, transmission; Prevalence; Serogroup},
