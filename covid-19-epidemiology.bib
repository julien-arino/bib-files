  issn-linking    = {1877-5845},
  keywords        = {Communicable Diseases, Emerging, epidemiology; Coronavirus Infections, diagnosis, epidemiology; Databases, Factual; Disease Outbreaks, statistics & numerical data; Female; Humans; Male; Mass Screening, methods; Models, Statistical; Monte Carlo Method; Pandemics, statistics & numerical data; Pneumonia, Viral, diagnosis, epidemiology; Poisson Distribution; Prevalence; Prospective Studies; Public Health; Severe Acute Respiratory Syndrome, diagnosis, epidemiology; Space-Time Clustering; United States, epidemiology; COVID-19; Disease surveillance; Pandemic; SaTScan; Space-time clusters},
  nlm-id          = {101516571},
