@Article{LiWangHuangLu2020,
  author          = {Li, Zhihui and Wang, Jin and Huang, Jingyi and Lu, Jiahai},
  date            = {2020-08},
  journaltitle    = {Journal of infection in developing countries},
  title           = {Epidemiological characteristics of COVID-19 in Shenzhen, China: comparison between imported and local cases.},
  doi             = {10.3855/jidc.12801},
  issn            = {1972-2680},
