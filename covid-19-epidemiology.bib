  completed       = {2020-09-17},
  country         = {England},
  issn-linking    = {0143-005X},
  keywords        = {Betacoronavirus; China; Contact Tracing; Coronavirus; Coronavirus Infections, epidemiology, prevention & control; Disease Outbreaks, prevention & control; Humans; Infection Control; Models, Theoretical; Pandemics, prevention & control; Pneumonia, Viral, epidemiology, prevention & control; Public Health; Disease modeling; Epidemiology; communicable diseases; public health policy},
