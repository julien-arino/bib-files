@Article{RibeiroCastroESilvaDattiloReisEtAl2020,
  author       = {Ribeiro, Sérvio Pontes and Castro E Silva, Alcides and Dáttilo, Wesley and Reis, Alexandre Barbosa and Góes-Neto, Aristóteles and Alcantara, Luiz Carlos Junior and Giovanetti, Marta and Coura-Vital, Wendel and Fernandes, Geraldo Wilson and Azevedo, Vasco Ariston C.},
