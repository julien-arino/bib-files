  author          = {Xu, Chengdong and Wang, Jinfeng and Wang, Li and Cao, Chunxiang},
  date            = {2014-12},
  journaltitle    = {BMC infectious diseases},
  title           = {Spatial pattern of severe acute respiratory syndrome in-out flow in 2003 in Mainland China.},
