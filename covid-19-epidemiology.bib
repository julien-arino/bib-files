@Article{QuarantaFormicaMachadoLacarbonaraEtAl2020,
  author       = {Quaranta, Giuseppe and Formica, Giovanni and Machado, J. Tenreiro and Lacarbonara, Walter and Masri, Sami F.},
  date         = {2020-09},
