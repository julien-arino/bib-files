  author          = {Barrett, Kali and Khan, Yasin A. and Mac, Stephen and Ximenes, Raphael and Naimark, David M. J. and Sander, Beate},
  date            = {2020-06},
  journaltitle    = {CMAJ : Canadian Medical Association journal = journal de l'Association medicale canadienne},
