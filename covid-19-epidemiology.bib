@Article{YamagishiKamiyaKakimotoSuzukiEtAl2020,
  author          = {Yamagishi, Takuya and Kamiya, Hajime and Kakimoto, Kensaku and Suzuki, Motoi and Wakita, Takaji},
  date            = {2020-06},
