  country         = {England},
  issn-linking    = {0022-5193},
  keywords        = {Betacoronavirus; China; Communicable Diseases, Emerging, epidemiology; Coronavirus Infections, epidemiology; Disease Outbreaks; Humans; Mass Media; Models, Theoretical; Pandemics; Pneumonia, Viral, epidemiology; COVID-19; Correlation analysis; Individual behaviour change; Media report; SEIR-type model},
