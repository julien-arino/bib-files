  completed       = {2020-07-30},
  country         = {Bosnia and Herzegovina},
  issn-linking    = {1840-0132},
  keywords        = {Betacoronavirus; Bosnia and Herzegovina, epidemiology; Communicable Disease Control; Coronavirus Infections, epidemiology, prevention & control, transmission; Data Visualization; Epidemics; Epidemiological Monitoring; Geographic Mapping; Health Information Systems; Humans; Models, Statistical; Pandemics, prevention & control; Pneumonia, Viral, epidemiology, prevention & control, transmission; Spatio-Temporal Analysis; COVID 19 pandemic; decision making; epidemiological techniques; geocoding; health information systems; reproduction number},
