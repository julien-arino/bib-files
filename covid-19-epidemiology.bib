@Article{OngChenCookLeeEtAl2010,
  author          = {Ong, Jimmy Boon Som and Chen, Mark I.-Cheng and Cook, Alex R. and Lee, Huey Chyi and Lee, Vernon J. and Lin, Raymond Tzer Pin and Tambyah, Paul Ananth and Goh, Lee Gan},
