  author          = {Lin, Chih-Yen and Wang, Wen-Hung and Urbina, Aspiro Nayim and Tseng, Sung-Pin and Lu, Po-Liang and Chen, Yen-Hsu and Yu, Ming-Lung and Wang, Sheng-Fan},
