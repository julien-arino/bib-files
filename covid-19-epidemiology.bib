@Article{MukhsamJeffreePangSyedAbdulRahimEtAl2020,
  author          = {Mukhsam, Mohamad Hafiz and Jeffree, Mohammad Saffree and Pang, Nicholas Tze Ping and Syed Abdul Rahim, Syed Sharizman and Omar, Azizan and Abdullah, Muhammad Syafiq and Lukman, Khamisah Awang and Giloi, Nelbon and Salvaraji, Loganathan and Abd Karim, Mohd Rahimie and Saupin, Sahipudin and Boon Tat, Yeap and Mohd Hayati, Mohd Firdaus and Ibrahim, Mohd Yusof and Muhamad, Assikin and Zainudin, Syaza Putri},
  date            = {2020-09},
  journaltitle    = {The American journal of tropical medicine and hygiene},
  title           = {A University-Wide Preparedness Effort in the Alert Phase of COVID-19 Incorporating Community Mental Health and Task-Shifting Strategies: Experience from a Bornean Institute of Higher Learning.},
  doi             = {10.4269/ajtmh.20-0458},
  issn            = {1476-1645},
