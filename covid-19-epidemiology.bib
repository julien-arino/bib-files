  issn-linking    = {0042-9686},
  keywords        = {Aircraft, legislation & jurisprudence; Airports, standards; Betacoronavirus; Communicable Disease Control, methods; Coronavirus Infections, epidemiology, prevention & control, transmission; Humans; Models, Statistical; Pandemics, prevention & control; Pneumonia, Viral, epidemiology, prevention & control, transmission; Risk Assessment; Time Factors; Travel, legislation & jurisprudence},
