@Article{WuLeungLeung2020,
  author          = {Wu, Joseph T. and Leung, Kathy and Leung, Gabriel M.},
  date            = {2020-02},
  journaltitle    = {The Lancet},
  title           = {Nowcasting and forecasting the potential domestic and international spread of the 2019-{nCoV} outbreak originating in {W}uhan, {C}hina: a modelling study.},
