  keywords        = {Disease Outbreaks; Global Health; Humans; Lassa Fever, epidemiology, transmission; Nigeria, epidemiology; Seasons; Travel},
  nlm-id          = {0370507},
  owner           = {NLM},
  pmc             = {PMC6402922},
