  pii             = {S0013-9351(20)31067-7},
  pmid            = {32950514},
  pubmodel        = {Print-Electronic},
  revised         = {2020-09-20},
}

@Article{BicharaIggidr2018,
  author          = {Bichara, Derdei and Iggidr, Abderrahman},
  date            = {2018-07},
