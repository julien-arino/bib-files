  keywords        = {Betacoronavirus; Coronavirus Infections; Data Accuracy; Disease Outbreaks; Forecasting; Humans; Machine Learning; Models, Statistical; Pandemics; Pneumonia, Viral; And machine learning; COVID-19; Data visualization; Prediction model},
  nlm-id          = {7806056},
  owner           = {NLM},
  pii             = {10.1007/s10916-020-01645-z},
  pmc             = {PMC7425790},
  pmid            = {32794042},
