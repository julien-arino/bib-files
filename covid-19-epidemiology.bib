  country         = {United States},
  issn-linking    = {1473-3099},
  keywords        = {Communicable Diseases, epidemiology; Crowding; Disease Outbreaks, prevention & control; Disease Transmission, Infectious, prevention & control; Global Health; Humans; Influenza A Virus, H1N1 Subtype; Influenza, Human, prevention & control; Models, Theoretical; Population Surveillance; Travel},
