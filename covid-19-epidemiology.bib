  completed       = {2020-09-02},
  country         = {England},
  file            = {:CliffordPearsonKlepacVanZandvoortEtAl2020 - Effectiveness of Interventions Targeting Air Travellers for Delaying Local Outbreaks of SARS CoV 2..pdf:PDF},
  issn-linking    = {1195-1982},
  keywords        = {Air Travel; Betacoronavirus; Coronavirus Infections, prevention & control, transmission; Humans; Mass Screening, standards; Pandemics, prevention & control; Pneumonia, Viral, prevention & control, transmission; Time Factors; Coronavirus; covid-19; public health emergency of international concern; travel screening},
