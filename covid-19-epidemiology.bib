  keywords        = {Betacoronavirus; Coronavirus Infections, epidemiology, transmission; Geographic Information Systems; Humans; Pandemics; Pneumonia, Viral, epidemiology, transmission; Public Health; COVID-19; GIS; SARS-CoV-2},
  nlm-id          = {101152198},
