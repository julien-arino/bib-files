@Article{AbdollahiHaworthBrockmanKeynanLangleyEtAl2020,
  author          = {Abdollahi, Elaheh and Haworth-Brockman, Margaret and Keynan, Yoav and Langley, Joanne M. and Moghadas, Seyed M.},
  date            = {2020-07},
  journaltitle    = {BMC medicine},
  title           = {Simulating the effect of school closure during COVID-19 outbreaks in Ontario, Canada.},
  doi             = {10.1186/s12916-020-01705-8},
  issn            = {1741-7015},
  issue           = {1},
  pages           = {230},
  pubstate        = {epublish},
  volume          = {18},
  abstract        = {The province of Ontario, Canada, has instituted indefinite school closures (SC) as well as other social distancing measures to mitigate the impact of the novel coronavirus disease 2019 (COVID-19) pandemic. We sought to evaluate the effect of SC on reducing attack rate and the need for critical care during COVID-19 outbreaks, while considering scenarios with concurrent implementation of self-isolation (SI) of symptomatic cases. We developed an age-structured agent-based simulation model and parameterized it with the demographics of Ontario stratified by age and the latest estimates of COVID-19 epidemiologic characteristics. Disease transmission was simulated within and between different age groups by considering inter- and intra-group contact patterns. The effect of SC of varying durations on the overall attack rate, magnitude and peak time of the outbreak, and requirement for intensive care unit (ICU) admission in the population was estimated. Secondly, the effect of concurrent community-based voluntary SI of symptomatic COVID-19 cases was assessed. SC reduced attack rates in the range of 7.2-12.7% when the duration of SC increased from 3 to 16 weeks, when contacts among school children were restricted by 60-80%, and in the absence of SI by mildly symptomatic persons. Depending on the scenario, the overall reduction in ICU admissions attributed to SC throughout the outbreak ranged from 3.3 to 6.7%. When SI of mildly symptomatic persons was included and practiced by 20%, the reduction of attack rate and ICU admissions exceeded 6.3% and 9.1% (on average), respectively, in the corresponding scenarios. Our results indicate that SC may have limited impact on reducing the burden of COVID-19 without measures to interrupt the chain of transmission during both pre-symptomatic and symptomatic stages. While highlighting the importance of SI, our findings indicate the need for better understanding of the epidemiologic characteristics of emerging diseases on the effectiveness of social distancing measures.},
  citation-subset = {IM},
  completed       = {2020-08-03},
  country         = {England},
  file            = {:AbdollahiHaworthBrockmanKeynanLangleyEtAl2020 - Simulating the Effect of School Closure during COVID 19 Outbreaks in Ontario, Canada..pdf:PDF},
  issn-linking    = {1741-7015},
  keywords        = {Betacoronavirus, pathogenicity; Child; Coronavirus, pathogenicity; Coronavirus Infections, epidemiology, transmission; Humans; Ontario, epidemiology; Pandemics; Pneumonia, Viral, epidemiology, transmission; Schools, statistics & numerical data; COVID-19; Pandemic; School closure; Self-isolation; Simulation; Social distancing},
  nlm-id          = {101190723},
  owner           = {NLM},
  pii             = {10.1186/s12916-020-01705-8},
  pmc             = {PMC7378981},
  pmid            = {32709232},
  pubmodel        = {Electronic},
  revised         = {2020-08-03},
}

@Article{AbdulaalPatelCharaniDennyEtAl2020,
  author          = {Abdulaal, Ahmed and Patel, Aatish and Charani, Esmita and Denny, Sarah and Mughal, Nabeela and Moore, Luke},
  date            = {2020-08},
  journaltitle    = {Journal of medical Internet research},
  title           = {Prognostic Modeling of COVID-19 Using Artificial Intelligence in the United Kingdom: Model Development and Validation.},
  doi             = {10.2196/20259},
  issn            = {1438-8871},
  issue           = {8},
  pages           = {e20259},
  pubstate        = {epublish},
  volume          = {22},
  abstract        = {The current severe acute respiratory syndrome coronavirus 2 (SARS-CoV-2) outbreak is a public health emergency and the case fatality rate in the United Kingdom is significant. Although there appear to be several early predictors of outcome, there are no currently validated prognostic models or scoring systems applicable specifically to patients with confirmed SARS-CoV-2. We aim to create a point-of-admission mortality risk scoring system using an artificial neural network (ANN). We present an ANN that can provide a patient-specific, point-of-admission mortality risk prediction to inform clinical management decisions at the earliest opportunity. The ANN analyzes a set of patient features including demographics, comorbidities, smoking history, and presenting symptoms and predicts patient-specific mortality risk during the current hospital admission. The model was trained and validated on data extracted from 398 patients admitted to hospital with a positive real-time reverse transcription polymerase chain reaction (RT-PCR) test for SARS-CoV-2. Patient-specific mortality was predicted with 86.25% accuracy, with a sensitivity of 87.50% (95% CI 61.65%-98.45%) and specificity of 85.94% (95% CI 74.98%-93.36%). The positive predictive value was 60.87% (95% CI 45.23%-74.56%), and the negative predictive value was 96.49% (95% CI 88.23%-99.02%). The area under the receiver operating characteristic curve was 90.12%. This analysis demonstrates an adaptive ANN trained on data at a single site, which demonstrates the early utility of deep learning approaches in a rapidly evolving pandemic with no established or validated prognostic scoring systems.},
  citation-subset = {IM},
  completed       = {2020-09-04},
  country         = {Canada},
  issn-linking    = {1438-8871},
  keywords        = {Aged; Aged, 80 and over; Artificial Intelligence; Betacoronavirus; Comorbidity; Coronavirus Infections, diagnosis, epidemiology; Female; Hospitalization; Humans; Male; Middle Aged; Neural Networks, Computer; Pandemics; Pneumonia, Viral, diagnosis, epidemiology; Prognosis; ROC Curve; United Kingdom; COVID-19; artificial intelligence; coronavirus; deep learning; machine learning; modeling; neural network; prediction},
  nlm-id          = {100959882},
