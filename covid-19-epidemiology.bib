  completed       = {2020-05-20},
  country         = {Canada},
  issn-linking    = {1201-9712},
  keywords        = {Betacoronavirus; China, epidemiology; Cities; Coronavirus Infections, epidemiology; Disease Outbreaks; Epidemics; Humans; Pandemics; Pneumonia, Viral, epidemiology; COVID-19; China; Spatial analysis; Spatial autocorrelation},
