  author          = {He, Daihai and Lui, Roger and Wang, Lin and Tse, Chi Kong and Yang, Lin and Stone, Lewi},
  date            = {2015-06},
  journaltitle    = {Scientific reports},
  title           = {Global Spatio-temporal Patterns of Influenza in the Post-pandemic Era.},
