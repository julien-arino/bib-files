@Article{Gillespie2009,
  author      = {Gillespie, Dan T. and Cao, Yang and Sanft, Kevin R. and Petzold, Linda R.},
  journal     = {J Chem Phys},
  title       = {The subtle business of model reduction for stochastic chemical kinetics.},
  year        = {2009},
  month       = {Feb},
  number      = {6},
  pages       = {064103},
  volume      = {130},
  abstract    = {This paper addresses the problem of simplifying chemical reaction networks by adroitly reducing the number of reaction channels and chemical species. The analysis adopts a discrete-stochastic point of view and focuses on the model reaction set S(1)<=>S(2)-->S(3), whose simplicity allows all the mathematics to be done exactly. The advantages and disadvantages of replacing this reaction set with a single S(3)-producing reaction are analyzed quantitatively using novel criteria for measuring simulation accuracy and simulation efficiency. It is shown that in all cases in which such a model reduction can be accomplished accurately and with a significant gain in simulation efficiency, a procedure called the slow-scale stochastic simulation algorithm provides a robust and theoretically transparent way of implementing the reduction.},
  doi         = {10.1063/1.3072704},
  keywords    = {Kinetics; Models, Chemical; Sensitivity and Specificity; Stochastic Processes},
  language    = {eng},
  medline-pst = {ppublish},
  owner       = {jarino},
  pmid        = {19222263},
  school      = {Dan T. Gillespie Consulting, 30504 Cordoba Place, Castaic, California 91384, USA. gillespiedt@mailaps.org},
  timestamp   = {2013.07.22},
  url         = {http://dx.doi.org/10.1063/1.3072704},
}

