  language    = {eng},
  medline-pst = {ppublish},
  owner       = {jarino},
  pmid        = {15606235},
  school      = {Department of Computer Science, University of California, Santa Barbara, California 93106, USA. ycao@cs.ucsb.edu},
  timestamp   = {2013.07.22},
