@Article{KoolMoilanenTreml2013,
  author	= {Kool, J.T. and Moilanen, A. and Treml, E.A.},
  journal	= {Landscape Ecology},
  title		= {Population connectivity: recent advances and new
		  perspectives},
  year		= {2013},
  issn		= {1572-9761},
  month		= feb,
  number	= {2},
  pages		= {165},
  volume	= {28},
  abstract	= {Connectivity is a vital component of metapopulation and
		  landscape ecology, influencing fundamental processes such
		  as population dynamics, evolution, and community responses
		  to climate change. Here, we review ongoing developments in
		  connectivity science, providing perspectives on recent
		  advances in identifying, quantifying, modelling and
		  analysing connectivity, and highlight new applications for
		  conservation. We also address ongoing challenges for
		  connectivity research, explore opportunities for addressing
		  them and highlight potential linkages with other fields of
		  research. Continued development of connectivity science
		  will provide insights into key aspects of ecology and the
		  evolution of species, and will also contribute
		  significantly towards achieving more effective conservation
		  outcomes.},
  date		= {2013-02-01},
  doi		= {10.1007/s10980-012-9819-z},
  groups	= {[jarino:]},
