@Article{ZhaoZhou2016,
  author	= {Zhao, Xiao-Qiang and Zhou, Peng},
  journal	= {Calculus of Variations and Partial Differential
		  Equations},
  title		= {On a Lotka-Volterra competition model: the effects of
		  advection and spatial variation},
  year		= {2016},
  issn		= {1432-0835},
  month		= jun,
  number	= {4},
  pages		= {1},
  volume	= {55},
  abstract	= {We consider a two-species Lotka-Volterra competition model
		  in a one-dimensional habitat where one species assumes pure
		  random diffusion while another one undergoes mixed movement
		  (both random and directed movements). If the environment is
		  spatially homogeneous, it has been confirmed in Lou et al.
		  (Discrete Contin. Dyn. Syst. A 36:953-969, 2016) that pure
		  random diffusion always wins. In this paper, we take into
		  account a more realistic situation where environmental
		  heterogeneity is involved. It turns out that the combined
		  effects of advection and spatial variations will bring much
		  richer phenomena: either pure random diffusion or mixed
		  movement wins, or both species can coexist eventually.},
  date		= {2016-06-18},
  doi		= {10.1007/s00526-016-1021-8},
  groups	= {[jarino:]},
  owner		= {jarino},
  publisher	= {Springer},
  timestamp	= {2016.08.02},
  url		= {http://dx.doi.org/10.1007/s00526-016-1021-8}
}
