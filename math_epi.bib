@Article{AndrewsMorrowWood2013,
  author	= {Andrews, J.R. and Morrow, C. and Wood, R.},
  journal	= {Am J Epidemiol},
  title		= {Modeling the role of public transportation in sustaining
		  tuberculosis transmission in {S}outh {A}frica.},
  year		= {2013},
  month		= {Mar},
  number	= {6},
  pages		= {556--561},
  volume	= {177},
  abstract	= {Current tuberculosis notification rates in South Africa
		  are among the highest ever recorded. Although the human
		  immunodeficiency virus epidemic has been a critical factor,
		  the density of respiratory contacts in high-risk
		  environments may be an important and underappreciated
		  driver. Using a modified Wells-Riley model for airborne
		  disease transmission, we estimated the risk of tuberculosis
		  transmission on 3 modes of public transit (minibus taxis,
		  buses, and trains) in Cape Town, South Africa, using
		  exhaled carbon dioxide as a natural tracer gas to evaluate
		  air exchange. Carbon dioxide measurements were performed
		  between October and December of 2011. Environmental risk,
		  reflected in the rebreathed fraction of air, was highest in
		  minibus taxis and lowest in trains; however, the average
		  number of passengers sharing an indoor space was highest in
		  trains and lowest in minibus taxis. Among daily commuters,
		  the annual risk of tuberculosis infection was projected to
		  be 3.5\%-5.0\% and was highest among minibus taxi
		  commuters. Assuming a duration of infectiousness of 1 year,
		  the basic reproductive number attributable to
		  transportation was more than 1 in all 3 modes of
		  transportation. Given its poor ventilation and high
