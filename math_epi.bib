@Article{ChowellDiaz-DuenasChowell2005,
  author	= {Gerardo Chowell and Porfirio Diaz-Dueñas and Diego
		  Chowell},
  journal	= {Scand J Infect Dis},
  title		= {The dynamics of pulmonary tuberculosis in Colima, Mexico
