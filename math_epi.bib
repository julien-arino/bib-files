  pii		= {1476-511X-10-187},
  pmid		= {22018353},
  school	= {Division of Pharmacoepidemiology and Clinical
		  Pharmacology, Utrecht Institute for Pharmaceutical
		  Sciences, Utrecht University, P.O. Box 80082, 3508 TB
		  Utrecht, The Netherlands.},
  timestamp	= {2013.09.23},
  url		= {http://dx.doi.org/10.1186/1476-511X-10-187}
}

