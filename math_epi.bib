@Article{DhirasakdanonThiemeDen2007,
  author	= {Dhirasakdanon, T. and Thieme, H.R. and van Den Driessche,
		  P.},
  journal	= {J Biol Dyn},
  title		= {A sharp threshold for disease persistence in host
		  metapopulations},
  year		= {2007},
  month		= {Oct},
  number	= {4},
  pages		= {363--378},
  volume	= {1},
  abstract	= {A sharp threshold is established that separates disease
		  persistence from the extinction of small disease outbreaks
		  in an S?E?I?R?S type metapopulation model. The travel rates
		  between patches depend on disease prevalence. The threshold
		  is formulated in terms of a basic replacement ratio
		  (disease reproduction number), ?(0), and, equivalently, in
		  terms of the spectral bound of a transmission and travel
		  matrix. Since frequency-dependent (standard) incidence is
		  assumed, the threshold results do not require knowledge of
		  a disease-free equilibrium. As a trade-off, for ?(0)>1,
		  only uniform weak disease persistence is shown in general,
		  while uniform strong persistence is proved for the special
		  case of constant recruitment of susceptibles into the patch
		  populations. For ?(0)<1, Lyapunov's direct stability method
		  shows that small disease outbreaks do not spread much and
		  eventually die out.},
  doi		= {10.1080/17513750701605465},
  institution	= {Department of Mathematics and Statistics, Arizona State
		  University, Tempe, AZ 85287-1804, USA.},
  keywords	= {Animals; Communicable Diseases, epidemiology; Disease
		  Outbreaks, statistics /&/ numerical data; Disease Vectors;
		  Endemic Diseases, statistics /&/ numerical data; Humans;
		  Models, Biological; Population Dynamics},
  language	= {eng},
