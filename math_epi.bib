  pmid		= {17376383},
  school	= {Infection Prevention and Control Unit, University Health
		  Network, Toronto, Ontario, Canada.},
  timestamp	= {2013.10.09},
  url		= {http://dx.doi.org/10.1016/S1473-3099(07)70029-4}
}

