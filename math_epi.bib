@Article{BerezovskayaWirkusSongCastillo-Chavez2011,
  Title                    = {Dynamics of population communities with prey migrations and Allee effects: a bifurcation approach.},
  Author                   = {Berezovskaya, Faina and Wirkus, S. and Song, B. and Castillo-Chavez, C.},
