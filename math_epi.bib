  pii		= {S1473-3099(13)70025-2},
  pmid		= {23531393},
  school	= {Global Alliance for TB Drug Development, New York, NY,
		  USA.},
  timestamp	= {2013.09.23},
  url		= {http://dx.doi.org/10.1016/S1473-3099(13)70025-2}
}

