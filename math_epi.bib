}

@Article{ShahWrightBaiBarreraEtAl2007,
  author	= {Shah, N.S. and Wright, A. and Bai, G.-H. and Barrera, L.
		  and Boulahbal, F. and Mart\'in-Casabona, N. and
		  Drobniewski, F. and Gilpin, C. and Havelkov\'a, M. and
