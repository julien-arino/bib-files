@Article{MagombedzeGariraMwenje2006,
  Title                    = {Modelling the human immune response mechanisms to mycobacterium tuberculosis infection in the lungs.},
  Author                   = {Gesham Magombedze and Winston Garira and Eddie Mwenje},
