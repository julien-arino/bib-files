@Article{RobinsonStilianakisDrossinos2012,
  author	= {Robinson, Marguerite and Stilianakis, Nikolaos I. and
		  Drossinos, Yannis},
  journal	= {Journal of Theoretical Biology},
  title		= {Spatial dynamics of airborne infectious diseases.},
  year		= {2012},
  month		= {Mar},
  pages		= {116--126},
  volume	= {297},
  abstract	= {Disease outbreaks, such as those of Severe Acute
		  Respiratory Syndrome in 2003 and the 2009 pandemic A(H1N1)
		  influenza, have highlighted the potential for airborne
		  transmission in indoor environments. Respirable
		  pathogen-carrying droplets provide a vector for the spatial
		  spread of infection with droplet transport determined by
		  diffusive and convective processes. An epidemiological
		  model describing the spatial dynamics of disease
		  transmission is presented. The effects of an ambient
		  airflow, as an infection control, are incorporated leading
		  to a delay equation, with droplet density dependent on the
		  infectious density at a previous time. It is found that
		  small droplets (∼0.4μm) generate a negligible infectious
		  force due to the small viral load and the associated
		  duration they require to transmit infection. In contrast,
		  larger droplets (∼4μm) can lead to an infectious wave
		  propagating through a fully susceptible population or a
		  secondary infection outbreak for a localized susceptible
		  population. Droplet diffusion is found to be an inefficient
		  mode of droplet transport leading to minimal spatial spread
		  of infection. A threshold air velocity is derived, above
		  which disease transmission is impaired even when the basic
		  reproduction number R(0) exceeds unity.},
  doi		= {10.1016/j.jtbi.2011.12.015},
  institution	= {Joint Research Centre, European Commission, I-21027 Ispra
		  (VA), Italy. marguerite.robinson@jrc.ec.europa.eu},
  keywords	= {Aerosols; Air Microbiology; Air Movements; Air Pollution,
		  Indoor, adverse effects/analysis; Diffusion; Disease
		  Outbreaks; Exhalation; Humans; Influenza, Human,
		  epidemiology/prevention /&/ control/transmission;
		  Inhalation Exposure, adverse effects/analysis; Models,
		  Biological; Particle Size; Ventilation},
  language	= {eng},
  medline-pst	= {ppublish},
  owner		= {jarino},
  pii		= {S0022-5193(11)00632-1},
  pmid		= {22207025},
  timestamp	= {2014.07.28},
  url		= {http://dx.doi.org/10.1016/j.jtbi.2011.12.015}
}

